#!/usr/bin/env groovy

node {
    stage('Build') {
        script {
            echo "-------------------------------------------------------"
            echo "Building full cURL string:"
            def response = sh(script: 'curl -X POST -s http://localhost:7234/workflows/v1/definitions/ -F taskService="TMSv1" -F bpmnFile=""', returnStdout: true)
            echo response
            echo "-------------------------------------------------------"
        }
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}