node {
    stage('Build') {
        script {
            echo "-------------------------------------------------------"
            echo "Building full cURL string:"
            curl -X POST -s http://localhost:7234/workflows/v1/definitions/ -F taskService="TMSv1" -F bpmnFile=""
            echo "-------------------------------------------------------"
        }
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}